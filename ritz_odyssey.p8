function init_ritz()
 ritz = {x=tile*(1), x_s=0,
         y=tile*5, y_s=0, y_m=4,
         w=8, h=8,
         stam=100, stam_s=0,
         running=false,
         jumping=false,
         falling=false,
         landed=false,
         alive=true,
         breaking=false,
         anim_time=0.3,
         frame=0,
         run_f=2,
         die_f=5,
         g=0.3,
         j_f=5}
end

function do_ritz()
 if ritz.alive then
		ritz_physics()
		if game_start then
 	 control_ritz()
 	end
	end
 ritz.frame = (time()/ritz.anim_time)%2
end

function ritz_physics()
 ritz.y_s+=ritz.g

 if game_start then
  ritz.running=true
 end
 
 if ritz.y_s > 0 then
  ritz.falling=true
  ritz.landed=false
  ritz.jumping=false
  if collide_map(ritz,"down",0) then
   ritz.landed = true
   ritz.falling = false
   ritz.y_s=0
   ritz.y-=((ritz.y+ritz.h+1)%8)-1
  end
 end
 if ritz.y_s < 0 then
  ritz.jumping=true
  if collide_map(ritz,"up",0) then
   ritz.y_s=0
  end
 end
 
 if ritz.y_s != 0 and
    game_start    and
    ritz.x_s == 0 then
  ritz.x_s = 1
 end
 
 if ritz.x_s > 0 then
  if collide_map(ritz,"right",0) then
   ritz.x_s=0
   ritz.running=false
  end
 end
   
 if ritz.y_s > ritz.y_m then
  ritz.y_s = ritz.y_m
 end
 
 ritz.y+=ritz.y_s
 ritz.x+=ritz.x_s
 ritz.stam-=ritz.stam_s
 
 if ritz.stam <= 0 then
  ritz.stam = 0
  kill_ritz()
 end
 
 if ritz.y >= 15*tile then
  kill_ritz()
 end
end

function draw_ritz()
 if not ritz.alive then
  spr(ritz.frame + ritz.die_f, ritz.x, ritz.y)
	 spr(17, ritz.x+7, ritz.y-2)
 else
	 if ritz.jumping or ritz.falling then
	  spr(4, ritz.x, ritz.y)
	 	spr(17, ritz.x+6, ritz.y-2)
	 elseif ritz.breaking then
	  spr(7, ritz.x, ritz.y)
	 	spr(17, ritz.x+6, ritz.y-2)
	 elseif ritz.running then
	  spr(ritz.frame + ritz.run_f, ritz.x, ritz.y)
	 	spr(18, ritz.x+6, ritz.y)
	 else
	  spr(1, ritz.x, ritz.y)
	 	spr(17, ritz.x+6, ritz.y)
	 end
	 if ritz.dashing then
	  local l_x = ritz.x-rnd(4)
	  local l_y = ritz.y+rnd(8)
	  line(l_x,l_y,l_x+4,l_y,7)
	 end
	end
end

function kill_ritz()
 sfx(6)
 ritz.alive = false
 restart_time=time()+3
end

