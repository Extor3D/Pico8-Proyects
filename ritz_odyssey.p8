--util code

function init_util()
 t_box = 0
 t_baj = 0
 t_sub = 0
end

function collide_map(obj,aim,flag)
 local x=obj.x    local y=obj.y
 local w=obj.w    local h=obj.h
 
 local x1=0    local y1=0
 local x2=0    local y2=0
 
 if aim=="left" then
   x1=x-1  y1=y
   x2=x    y2=y+h-1

 elseif aim=="right" then
   x1=x+w    y1=y
   x2=x+w+1  y2=y+h-1

 elseif aim=="up" then
   x1=x+1    y1=y-1
   x2=x+w-1  y2=y

 elseif aim=="down" then
   x1=x      y1=y+h
   x2=x+w    y2=y+h
 end
 
 x1/=8    y1/=8
 x2/=8    y2/=8
 
 if x1 > 8*16 then
  x1-=8*16
  y1+=16
 end
 if x2 > 8*16 then
  x2-=8*16
  y2+=16
 end
 
 if fget(mget(x1,y1), flag)
 or fget(mget(x1,y2), flag)
 or fget(mget(x2,y1), flag)
 or fget(mget(x2,y2), flag) then
    return true
 else
    return false
 end
end

function hit(obj1, obj2)
 if obj1.y > obj2.y+obj2.h then
  return false
 end
 if obj1.y+obj1.h < obj2.y then
  return false
 end
 if obj1.x > obj2.x+obj2.w then
  return false
 end
 if obj1.x+obj1.w < obj2.x then
  return false
 end
 
 return true
end

function copy_table(table)
 local t = {}
 for key, value in pairs(table) do
  t[key] = value
 end
 return t
end

function print_box(t,x,y,tim)

 local f = 0

 if t_box < time() then
  t_box = time() + tim
  t_baj = time() + (tim / 6)
  t_sub = time() + (tim * (5/6))
 end
 
 if time() < t_baj then
  f = (t_baj - time()) * -1
 elseif time() > t_sub then
  f = (time() - t_sub) * -1
 end
 
 y += (f * y * 4)
 
 palt(0, false)
	palt(14,true)
		
 local box_x = 56
 local box_y = 16
 local len = #t * 4 + 7
 sspr(box_x+3,box_y+3,2,2,x-4,y-2,len,9)
 sspr(box_x,box_y,3,3,x-5,y-4)
 sspr(box_x+5,box_y,3,3,x+len-6,y-4)
 sspr(box_x+5,box_y+5,3,3,x+len-6,y+6)
 sspr(box_x,box_y+5,3,3,x-5,y+6)
 
 sspr(box_x+3,box_y+5,2,3,x-2,y+6,len-4,3)
 sspr(box_x+3,box_y,2,3,x-2,y-4,len-4,3)
 sspr(box_x,box_y+3,3,2,x-5,y-1,3,7)
 sspr(box_x+5,box_y+3,3,2,x+len-6,y-1,3,7)

 palt()
 print(t,x,y,7)
 
end
