function _update()
 if logo_on and 
    time() > logo_time then
  logo_on = false
  splash_on = true
  music(0)
 end
 
 if splash_on and btn(âŽ) then
  splash_on = false
  game_on = true
  init_game()
  music(-1)
 end 

 if game_on and not quest_complete then
	 if time() > intro_time then
	  game_start = true
	 end
	 if time() == restart_time then
	  init_game()
	 end
	 if ritz.alive and ritz.x > end_x then
 	 sfx(7)
	  quest_complete = true
	  quest_time = time()+3
	 end
	 
	 spawn_enemies()
	 spawn_items()
	 control_enemies()
	 control_items()
	 do_ritz()
	end
end

function _draw()
	cls()
	
	if logo_on then
	 draw_logo()
	end
	
	if splash_on then
	 draw_splash()
	end
	
	if game_on then	 
	 local cam_x = ritz.x
	 if ritz.x > 240*tile then
	  cam_x = 240*tile
	 end
		camera(cam_x-tile,0)
		
		if quest_complete then
	  mset(124,26,42)
	  mset(124,27,58)
 	end
		
		draw_map()
		draw_ritz()
		draw_enemies()
		draw_items()
		draw_hud(cam_x)
		if time() < intro_time then
	  print_box("time limit: 50 min.", 25, 60, 3)
	 end
	 if time() < restart_time then
	  print_box("quest failed", cam_x + 30, 60, 3)
	 end
	 if time() < quest_time then
	  print_box("quest completed!", cam_x + 20, 60, 3)
	 end
	end
	
	if quest_complete and 
	   time() > quest_time then
	 cls()
	 camera(0,0)
	 print_box("thanks for playing!", 25, 60, 10)
	end
end

