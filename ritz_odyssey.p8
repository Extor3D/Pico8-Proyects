function init_enemies()
 bull = {x=0, x_s=0,
         y=0, y_s=0, y_m=4,
         w=8, h=8,
         running=false,
         falling=false,
         landed=false,
         anim_time=0.3,
         g=0.3}
         
 u_ur = {x=0, x_s=0,
         y=0, y_s=0,
         w=8, h=16,
         hidden=true,
         finish=false,
         s_time=0,
         anim_time=0.3,
         sp=1.5}
         
 d_ur = {x=0, x_s=0,
         y=0, y_s=0,
         w=8, h=16,
         hidden=true,
         finish=false,
         s_time=0,
         anim_time=0.3,
         sp=0.5}
         
 h_ur = {x=0, x_s=0,
         y=0, y_s=0,
         w=16, h=8,
         shooting=false,
         right=false,
         wait_t=0,
         shoot_t=2}
         
 fball= {x=0, x_s=0,
         y=0, y_s=-2,
         w=8, h=8,
         right=false}
end

function spawn_enemies()
 for b in all(b_tiles) do
  if (ritz.x + tile*16) > (b.x * tile) then
   spawn_e_type(b, b_enemies, 
                b_tiles, bull)
  end
 end
 for u in all(u_tiles) do
  if (ritz.x + tile*16) > (u.x * tile) then
   local new = spawn_e_type(u, u_enemies, 
                  u_tiles, u_ur)
   new.s_time=time()
  end
 end
 for d in all(d_tiles) do
  if (ritz.x + tile*16) > (d.x * tile) then
   local new =spawn_e_type(d, d_enemies, 
                d_tiles, d_ur)
   new.s_time=time()
  end
 end
 for h in all(h_tiles) do
  if (ritz.x + tile*16) > (h.x * tile) then
   local new =spawn_e_type(h, h_enemies, 
                h_tiles, h_ur)
   new.right = h.right
  end
 end
end

function spawn_e_type(obj, t_e, t_t, table)
  local new = copy_table(table)
  new.x = obj.x * tile
  new.y = obj.y * tile
  add(t_e, new)
  del(t_t, obj)
  return new
end

function control_enemies()
 for b in all(b_enemies) do
  do_bull(b)
 end
 for u in all(u_enemies) do
  do_uro(u, "up")
 end
 for d in all(d_enemies) do
  do_uro(d, "down")
 end
 for h in all(h_enemies) do
  do_h_uro(h)
 end
 for f in all(fireballs) do
  do_fireball(f)
 end
end

function do_fireball(f)
 f.y_s+=0.1
 if f.right then
  f.x+=1
 else
  f.x-=1
 end
  f.y+=f.y_s
  
 if f.y > 16*tile then
  del(fireballs, f)
 end
 
 if hit(f, ritz) and 
    ritz.alive then
  kill_ritz()
 end
end

function do_h_uro(h)
 if h.shooting and 
    time() > h.wait_t then
  h.shooting = false
  h.wait_t = time() + h.shoot_t
 else 
  if time() > h.wait_t and
         not h.shooting then
	  h.shooting = true
	  h.wait_t = time() + 0.5
	  local temp_x = h.x - 4
	  if h.right then
	   temp_x = h.x + h.w
	  end
	  local new = copy_table(fball)
	  new.x = temp_x
	  new.y = h.y
	  new.right = h.right
	  add(fireballs, new)
	 end
	end
end

function do_uro(u,dir)
 local exit_time = u.s_time + 2
 
 if u.y == -tile or 
    (time() > exit_time and
     u.finish) then
  del(u_enemies, u)
 end
 
 if not u.hidden and
    collide_map(u,dir,0) and
    time() > exit_time then
  u.finish = true
  u.s_time = time()
 end
 
 if time() > exit_time and 
    u.hidden then
  u.hidden=false
  u.s_time = time() - 1.7
 end
 
 if not u.hidden and 
    not u.finish then
  if dir == "up" then
   u.y-=u.sp
  else
   u.y+=u.sp
  end
	 if hit(u, ritz) and 
	    ritz.alive then
	  kill_ritz()
	 end
 end 
end

function do_bull(b)
 b.x_s = -1
 b.y_s+=b.g
 
 if b.y_s > 0 then
  b.falling=true
  b.landed=false
  if collide_map(b,"down",0) then
   b.landed = true
   b.falling = false
   b.y_s=0
   b.y-=((b.y+b.h+1)%8)-1
  end
 end
 
 if b.x_s < 0 then
  if collide_map(b,"left",0) then
   b.x_s=0
  end
 end
 
 b.y+=b.y_s
 b.x+=b.x_s
 
 if hit(b, ritz) and ritz.alive then
  kill_ritz()
 end
 
 if b.x < ritz.x -30 then
  del(b_enemies, b)
 end
end

function draw_enemies()
 for b in all(b_enemies) do
  draw_bull(b)
 end
 for u in all(u_enemies) do
  draw_uro(u, false)
 end
 for d in all(d_enemies) do
  draw_uro(d, true)
 end
 for h in all(h_enemies) do
  draw_h_uro(h)
 end
 for f in all(fireballs) do
  draw_fball(f)
 end
end

function draw_fball(f)
 local frame = (time()/0.2)%2
 spr(frame + 37, f.x, f.y)
end

function draw_h_uro(h)
 if not h.shooting then
  sspr(1*tile, 3*tile,
       2*tile, tile,
       h.x, h.y,
       2*tile, tile,
       h.right, false)
 else
  sspr(3*tile, 3*tile,
       2*tile, tile,
       h.x, h.y,
       2*tile, tile,
       h.right, false)
 end
end

function draw_uro(u, f)
 local frame = flr((time()/u_ur.anim_time)%2)
 if not u.hidden and not u.finish then
  sspr((9+frame)*tile, 0,
       tile, 2*tile,
       u.x, u.y,
       tile, 2*tile,
       false, f)
 end
 if u.hidden then
  spr(35 + frame, u.x, u.y,1,1,false,f)
 end
 if u.finish then
  local bit = 0
  if f then
   bit = tile
  end
  spr(35 + frame, u.x, u.y + bit,1,1,false,not f)
 end
end

function draw_bull(b)
 local frame = 33+(time()/bull.anim_time)%2
 if b.x_s == 0 then
  spr(33, b.x, b.y)
 else
  spr(frame, b.x, b.y)
 end
end
