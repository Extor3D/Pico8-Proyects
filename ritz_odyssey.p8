function init_items()
 meat = {x=0, x_s=0,
         y=0, y_s=0,
         w=8, h=8,
         anim_time=0.3}
end

function spawn_items()
 for m in all(m_tiles) do
  if (ritz.x + tile*16) > (m.x * tile) then
   local new_m = copy_table(meat)
   new_m.x = m.x * tile
   new_m.y = m.y * tile
   add(m_items, new_m)
   del(m_tiles, m)
  end
 end
end

function control_items()
 for m in all(m_items) do
  if hit(m, ritz) and ritz.alive then
   del(m_items, m)
   ritz.stam+=50
   sfx(01)
   if ritz.stam > 100 then
    ritz.stam = 100
   end
  end
  if m.x < ritz.x -30 then
   del(m_items, m)
  end
 end
end

function draw_items()
 for m in all(m_items) do
  local frame = (time()/meat.anim_time)%2
  spr(19, m.x, m.y-frame)
 end
end
